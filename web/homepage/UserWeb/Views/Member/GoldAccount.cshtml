    }
    //올인지원금.
    function uf_RequestAllin() {
        var chk = confirm("올인지원금을 요청하시겠습니까?")
        if (chk == true) {
            document.getElementById('HiddenFrame').contentWindow.location = "/member/request_allin.asp";
        }
    }
    //개인올인포인트 전환
    function uf_RequestPoint() {
        var chk = confirm("올인포인트를 요청하시겠습니까?")
        if (chk == true) {
            document.getElementById('HiddenFrame').contentWindow.location = "/member/request_point.asp";
        }
